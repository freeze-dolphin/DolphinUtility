dolphin-util {
	help This command do not have any feature.
	
	fetch-url string:url string:targetPath {
		help Download a file and overwrite a local file with it. Notice that the path seperators in argument 'targetPath' should be '|'.
		permission dolphin-utility.access.fetch-url
		hook dolphin-util.fetch-url
	}
	
	config-reload {
		help Reload the config.
		permission dolphin-utility.access.reload-config
		hook dolphin-util.reload-config
	}
}
